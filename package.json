{
  "name": "@moruk/numy",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "engines": {
    "node": "24.12.0"
  },
  "scripts": {
    "android": "yarn run killport && expo run:android --port 2007",
    "build:android": "eas build --platform android --profile production --local",
    "build:ios": "eas build --platform ios --profile production --local",
    "clean": "sh -c 'rm -rf ~/Library/Developer/Xcode/DerivedData/Numy* ios/Pods ios/build android/build android/app/build' && npx expo prebuild --clean",
    "deploy:local": "yarn run killport && ./scripts/ios-build-submit.sh",
    "doctor": "npx expo-doctor",
    "doctor:fix": "npx expo install --fix && npx expo install --check",
    "fix": "yarn run lint:fix && yarn run format:fix && yarn run doctor:fix",
    "format": "yarn run format:fix",
    "format:fix": "npx prettier --write . --log-level error",
    "ios": "yarn run killport && expo run:ios --port 2007 --device \"iPad Pro 13-inch (M4)\"",
    "ios:fast": "yarn run killport && USE_CCACHE=1 expo run:ios --no-install --port 2007 --device \"iPad Pro 13-inch (M4)\"",
    "killport": "./scripts/killport.sh 2007",
    "lint": "yarn run lint:fix",
    "lint:fix": "expo lint --fix --quiet",
    "start": "yarn run killport && expo start --port 2007",
    "test": "jest --passWithNoTests --silent",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "type-check": "tsc --noEmit",
    "validate:metadata": "node scripts/localization/validate-metadata.js",
    "validate": "concurrently --kill-others-on-fail -n \"TypeCheck,Lint,Format,Doctor,Test\" -c \"magenta,cyan,yellow,magenta,purple\" \"yarn run type-check\" \"yarn run lint\" \"yarn run format\" \"yarn run doctor\" \"yarn run test\"",
    "watchman:clean": "watchman watch-del-all"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "*",
    "@react-native-firebase/analytics": "^23.8.4",
    "@react-native-firebase/app": "^23.8.4",
    "@react-native-firebase/crashlytics": "^23.8.4",
    "@react-native-firebase/remote-config": "^23.8.4",
    "@sentry/react-native": "~7.2.0",
    "@shopify/flash-list": "2.0.2",
    "decimal.js": "*",
    "expo": "~54.0.33",
    "expo-application": "~7.0.8",
    "expo-battery": "~10.0.8",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-localization": "~17.0.8",
    "expo-network": "~8.0.8",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-tracking-transparency": "^6.0.8",
    "expo-updates": "~29.0.16",
    "i18next": "*",
    "mathjs": "*",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "*",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^16.0.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "*",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "*",
    "react-native-worklets": "0.5.1",
    "tinyglobby": "^0.2.15",
    "zustand": "*"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@testing-library/dom": "*",
    "@testing-library/react-native": "*",
    "@types/jest": "29.5.14",
    "@types/node": "*",
    "@types/react": "~19.1.10",
    "babel-preset-expo": "~54.0.10",
    "concurrently": "*",
    "eslint": "*",
    "eslint-config-expo": "~10.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.17",
    "js-yaml": "^4.1.0",
    "prettier": "*",
    "react-refresh": "*",
    "react-test-renderer": "19.1.0",
    "sharp": "*",
    "ts-jest": "*",
    "typescript": "*",
    "typescript-eslint": "^8.52.0",
    "zod": "^3.23.8"
  },
  "private": true,
  "packageManager": "yarn@4.12.0"
}
