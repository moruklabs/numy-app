{
  "name": "@moruk/identifiers-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "engines": {
    "node": "25.2.1"
  },
  "scripts": {
    "prepare": "husky",
    "validate": "yarn workspaces foreach -Ap --exclude '@moruk/identifiers-monorepo' run validate",
    "validate:quick": "yarn type-check && yarn lint && yarn format",
    "validate:ci": "yarn validate:quick && yarn test",
    "validate:fix": "yarn fix:deps && yarn fix:expo",
    "test:app-config": "node app-config.test.js",
    "test:store-config": "node store-config.test.js",
    "test": "node app-config.test.js && node store-config.test.js && node localization.test.js && yarn workspaces foreach -Ap --exclude '@moruk/identifiers-monorepo' run test",
    "test:localization": "node localization.test.js",
    "test:apps": "yarn workspaces foreach -Ap --include '@moruk/*' --exclude '@moruk/identifiers-monorepo' run test",
    "test:coverage": "yarn workspaces foreach -Ap --include '@moruk/*' --exclude '@moruk/identifiers-monorepo' run test:coverage",
    "fix": "EXPO_NO_CACHE=1 yarn workspaces foreach -A --exclude '@moruk/identifiers-monorepo' run fix",
    "lint": "yarn lint:fix",
    "lint:fix": "yarn workspaces foreach -A --exclude '@moruk/identifiers-monorepo' run lint:fix",
    "format": "yarn run format:fix",
    "format:fix": "prettier --write .",
    "type-check": "yarn workspaces foreach -Ap --exclude '@moruk/identifiers-monorepo' run type-check",
    "build": "yarn workspaces foreach -Apt --exclude '@moruk/identifiers-monorepo' run build",
    "clean": "(yarn workspaces foreach -Ap --exclude '@moruk/identifiers-monorepo' run clean || true) && rm -rf node_modules",
    "killport": "bash scripts/killport.sh",
    "lint:expo": "npx expo install --check",
    "fix:expo": "npx expo install --fix",
    "doctor": "yarn workspaces foreach -Ap --exclude '@moruk/identifiers-monorepo' run doctor",
    "doctor:fix": "yarn workspaces foreach -A --exclude '@moruk/identifiers-monorepo' run doctor:fix",
    "expo:doctor": "npx expo-doctor",
    "watchman:clean": "watchman watch-del-all",
    "check:deps": "bash scripts/check-dependencies.sh"
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-test-renderer": "19.1.0",
